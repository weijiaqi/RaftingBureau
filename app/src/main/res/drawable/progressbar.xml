<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="4dp"/>
            <stroke android:width="0.4dp" android:color="@color/color_6de"/>

        </shape>
    </item>
    <item android:id="@android:id/progress" android:left="1.5dp" android:right="1.5dp" android:top="1dp" android:bottom="1dp">
        <scale
            android:drawable="@drawable/about_me_progress_finish"
            android:scaleWidth="100%" />
    </item>

</layer-list>