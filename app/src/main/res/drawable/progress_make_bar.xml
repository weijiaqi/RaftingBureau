<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="100dp" />
            <stroke
                android:width="2dp"
                android:color="@color/color_6dec" />
        </shape>
    </item>
    <item
        android:id="@android:id/secondaryProgress"
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="100dp" />
                <solid android:color="@color/color_6dec" />
            </shape>
        </scale>
    </item>

    <item
        android:id="@android:id/progress"
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <scale android:scaleWidth="100%" >
            <shape
                android:shape="rectangle">
                <solid android:color="@color/color_6dec" />
                <corners android:radius="100dp"/>
            </shape>
        </scale>
    </item>
</layer-list>
